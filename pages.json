{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{		//首页
			"path": "pages/index/index"
		}
	    ,{		//搜索
            "path" : "pages/search/search"
        }
        ,{		//预定指南
            "path" : "pages/reserve/reserve"
        }
		,{		//带驾服务
		    "path" : "pages/Service/Service"
		}
		,{		//客户服务
		    "path" : "pages/Customers/Customers"
		}
        ,{
            "path" : "pages/View/View"
            
        }
        ,{
            "path" : "pages/Service/Examine/Examine"
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"navigationStyle":"custom"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3A82F8",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/a.png",
			"selectedIconPath": "static/a-a.png",
			"text": "首页"
		},{
			"pagePath": "pages/reserve/reserve",
			"iconPath": "static/b.png",
			"selectedIconPath": "static/b-b.png",
			"text": "预定指南"
		},{
			"pagePath": "pages/Service/Service",
			"iconPath": "static/c.png",
			"selectedIconPath": "static/c-c.png",
			"text": "带驾服务"
		},{
			"pagePath": "pages/Customers/Customers",
			"iconPath": "static/d.png",
			"selectedIconPath": "static/d-d.png",
			"text": "客户服务"
		}
		]
	}
}
